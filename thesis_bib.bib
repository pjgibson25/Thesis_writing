@article{palotai_analysis_2018,
	title = {Analysis of June 2, 2016 bolide event},
	url = {http://arxiv.org/abs/1801.05072},
	abstract = {On June 2, 2016 at 10h56m {UTC}, a \$-18.9 {\textbackslash}pm 0.5\$ magnitude superbolide was observed over Arizona. We present analysis of this event based on 6 cameras and a multi-spectral sensor observations by the {SkySentinel} continuous fireball-monitoring camera network, supplemented by a dash cam footage and a fragmentation model. The bolide began its luminous flight at an altitude of \$104.8 {\textbackslash}pm 0.5\$ km at coordinates \${\textbackslash}phi = 34.59 {\textbackslash}pm 0.04{\textasciicircum}{\textbackslash}circ\$ N planetographic latitude, \${\textbackslash}lambda = 110.45 {\textbackslash}pm 0.04{\textasciicircum}{\textbackslash}circ\$ W longitude, and it had a pre-atmospheric velocity of \$17.6 {\textbackslash}pm 0.5\$ km/s. The calculated orbital parameters indicate that the meteoroid did not belong to any presently known asteroid family. From our calculations, the impacting object had an initial mass of \$11.4 {\textbackslash}pm 0.5\$ metric tonnes with an estimated initial size of \$1.89 {\textbackslash}pm 0.07\$ m.},
	journaltitle = {{arXiv}:1801.05072 [astro-ph]},
	author = {Palotai, Csaba and Sankar, Ramanakumar and Free, Dwayne L. and Howell, J. Andreas and Botella, Elena and Batcheldor, Daniel},
	urldate = {2018-04-08},
	date = {2018-01-15},
	eprinttype = {arxiv},
	eprint = {1801.05072},
	keywords = {Astrophysics - Earth and Planetary Astrophysics},
	file = {arXiv\:1801.05072 PDF:C\:\\Users\\pj\\Zotero\\storage\\MUWRYTBD\\Palotai et al. - 2018 - Analysis of June 2, 2016 bolide event.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\pj\\Zotero\\storage\\NINL5GZR\\1801.html:text/html}
}

