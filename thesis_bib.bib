@thesis{mcswain_using_2016,
	title = {Using An All-Sky Camera to Observe Fireballs and Characterize Near-Earth Meteoroids},
	url = {https://libmedia.willamette.edu/ds/discover/coll/e2e9de68-34a3-43f5-aee2-f02ad91b0abb/Rembold?selected=61331b15-c586-48e8-a281-629e3d267926&filter=none&pos=0&itype=i&offset=0},
	abstract = {Fireballs are bright objects caused when meteoroids burn up in the Earth's atmosphere. The use of all-sky cameras has become a notable method for amateur astronomers to gather and analyze fireball data. The primary goal of our all-sky camera was to be able to identify and record fireballs in an outdoor environment. This project focused on completing a build of an all-sky camera system and creating the supporting analysis software. We completed the majority of our initial build goals. However, there are still improvements that need to be made in order for our project to be self-contained. Our build primarily differs from previous all-sky camera builds through the use of a Raspberry Pi for onboard processing. Once we were content with our build and our software, we were able to collect 30 hours of data and detect 10 potential fireballs from the rooftop of Collins Science Building at Willamette University. Extensions to this project include understanding how many fireballs occur each night, their size, and where they are coming from. In order to do this, the videos that we extract will need to be analyzed for the number of events we are seeing, distribution of observed fireball location, and brightness. Completing each of these steps will allow for more efficient fireball monitoring, allowing for greater datasets to be gathered for better statistical population analysis.},
	institution = {Willamette University},
	type = {Physics Theses},
	author = {{McSwain}, Kyle},
	urldate = {2018-02-09},
	date = {2016-05-11},
	file = {Search Departmental Collections:C\:\\Users\\pj\\Zotero\\storage\\ZTAUBI3L\\Rembold.html:text/html}
}

